        <script type="text/javascript">
        function ShowSBrandVideo()
        {
           // $('body').css({'overflow':'hidden'});
            $('.dark_trailer, .dark_trailer .container').css({'width':540,'height': 290, 'margin-left': -277});
            $('#dark_trailer').css({'display':'block'});
            $('#container').html("<a class='png' href='#' onclick='HideSBrandVideo(); return false;'></a> <div id='fc_playerb120215' >Вам необходимо обновить Flash Player.</div>");
            $('.dark').css({'display':'block'}).animate({'opacity':0.7});
            getPlayer("http://tr.kinopoisk.ru/458/kinopoisk.ru-Monsters-Inc-120215.mp4","", 540, 290, true,"b120215");

        }
        
        function HideSBrandVideo()
        {
            $('body').css({'overflow':'auto'});
            $('.dark').animate({'opacity':0.6},function(){	$('.dark').css({'display':'none'});	});
            $('#dark_trailer').css({'display':'none'});
            $('#container').html("");
        }

        if(typeof(ShowBrandVideo)=='undefined')
        ShowBrandVideo=ShowSBrandVideo;
        </script>






