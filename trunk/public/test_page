        <script type="text/javascript">
        $(function () {
            var now = new Date();
            if (!$.cookie('mobile') || $.cookie('mobile') == '0' || $.cookie('mobile') == '1') {
                if (window.navigator && navigator.userAgent.toLowerCase().match(/(ipad|iphone|ipod).+os\ 6_/)) {
                    $.cookie('mobile', 'no', { expires: 365, path: '/', domain: '.kinopoisk.ru' });
                    return;
                }

                $.get('/handler_mobile_detector.php', {rnd: now.getTime()}, function (data) {
                    if (data == 'yes') {
                        $('#mobile_link').css('display', 'block');
                    }
                    $.cookie('mobile', data, { expires: 7, path: '/', domain: '.kinopoisk.ru' });
                });
            } else if ($.cookie('mobile') == 'yes') {
                $('#mobile_link').css('display', 'block');
            }
        });
        </script>
